{
  "name": "@joshuarileydev/coolify-mcp-server",
  "version": "1.0.5",
  "description": "MCP server for Coolify integration",
  "main": "dist/index.js",
  "bin": {
    "coolify-mcp-server": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    ".env.example"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoshuaRileyDev/coolify-mcp-server"
  },
  "scripts": {
    "build": "tsc && npx @modelcontextprotocol/inspector node dist/index.js",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "publish": "git add . && git commit -m 'update' && git push",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "coolify",
    "server",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@joshuarileydev/coolify-client": "^1.0.1",
    "@modelcontextprotocol/sdk": "^0.6.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
